@if (Debug)
{
    @if (DraggedObject != null)
    {
        <div class="alert alert-warning">
            DraggedObject is not null and of type @DraggedObjectType.
        </div>
    }
    else
    {
        <div class="alert alert-danger">
            DraggedObject is null!
        </div>
    }
}

<div class="d-flex gap-3 border border-dark p-3">
    <CascadingValue IsFixed="true" Value="this">
        @ChildContent
    </CascadingValue>
</div>